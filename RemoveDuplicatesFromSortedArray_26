class Solution(object):
    def removeDuplicates(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        length = 0
        nums_new = nums
        for key, number_i in enumerate(nums):
            if key == 0:
                nums_new[length] = number_i
                length += 1
            else:
                if nums_new[length-1] != number_i:
                    nums_new[length] = number_i
                    length += 1
        nums = nums_new
        return length

sol = Solution()
l = sol.removeDuplicates([1,2,2,3,3,3,4,5])
print l